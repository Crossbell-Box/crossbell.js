{
  "name": "crossbell.js",
  "version": "0.19.15",
  "description": "JavaScript SDK to interact with Crossbell",
  "packageManager": "pnpm@8.1.1",
  "keywords": [
    "crossbell",
    "eth",
    "contract",
    "web3"
  ],
  "bugs": {
    "url": "https://github.com/Crossbell-Box/crossbell.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Crossbell-Box/crossbell.js.git"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build-fast -- --dts-resolve",
    "postbuild": "node ./scripts/post-build.js",
    "build-fast": "tsup",
    "update-abi": "node ./scripts/update-abi.js",
    "docs:build": "typedoc",
    "docs:preview": "serve docs",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "watch": "npm run build-fast -- --watch"
  },
  "dependencies": {
    "@crossbell/ipfs-fetch": "^0.0.19",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "async-retry": "^1.3.3",
    "bn.js": "^5.2.1",
    "ethers": "^5.7.2",
    "fetch-undici": "^3.0.1",
    "p-limit": "^3.1.0",
    "ts-mixer": "^6.0.3",
    "web3-providers-ws": "1.9.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/async-retry": "^1.4.5",
    "@types/bn.js": "^5.1.1",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typechain": "^8.1.1",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
